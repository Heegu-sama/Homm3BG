"    \\item[\\textbf{1.}] Empty Field\n"
"    \\item[\\textbf{2.}] Location\n"
"    \\item[\\textbf{3.}] Artifact Symbol\n"
"    \\item[\\textbf{4.}] Blocked Field\n"
"    \\item[\\textbf{5.}] Border line\n"
"    \\item[\\textbf{6.}] Field Difficulty\n"
"  "
